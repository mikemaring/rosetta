<source>
  type tail
  format apache2
  path <%= node['rosetta']['agent']['apache_access_log'] %>
  tag rl.apache_access
  pos_file /var/log/td-agent/apache_access.pos
</source>

<match rl.**>
  type redislist
  host <%= node['rosetta']['broker']['redis_host'] %>
  port <%= node['rosetta']['broker']['redis_port'] %>
  database <%= node['rosetta']['broker']['redis_db'] %>
</match>
